<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   id="Definitions_1"
                   targetNamespace="http://bpmn.io/schema/bpmn">
  
  <bpmn:process id="Process_1" isExecutable="false">
    
    <!-- Lanes -->
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="lane1" name="Main">
        <bpmn:flowNodeRef>start1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>gw1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>gw2</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task5</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>end1</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="lane2" name="Shipping">
        <bpmn:flowNodeRef>task2</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="lane3" name="Gift Service">
        <bpmn:flowNodeRef>task3</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="lane4" name="Insurance">
        <bpmn:flowNodeRef>task4</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- Start Event -->
    <bpmn:startEvent id="start1" name="Start">
      <bpmn:outgoing>f1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Task: Receive Order -->
    <bpmn:task id="task1" name="Receive Order">
      <bpmn:incoming>f1</bpmn:incoming>
      <bpmn:outgoing>f2</bpmn:outgoing>
    </bpmn:task>
    
    <!-- Inclusive Gateway: Split (OR) -->
    <bpmn:inclusiveGateway id="gw1" name="Check Requirements">
      <bpmn:incoming>f2</bpmn:incoming>
      <bpmn:outgoing>f3</bpmn:outgoing>
      <bpmn:outgoing>f4</bpmn:outgoing>
      <bpmn:outgoing>f5</bpmn:outgoing>
    </bpmn:inclusiveGateway>
    
    <!-- Task: Standard Shipping -->
    <bpmn:task id="task2" name="Standard Shipping">
      <bpmn:incoming>f3</bpmn:incoming>
      <bpmn:outgoing>f6</bpmn:outgoing>
    </bpmn:task>
    
    <!-- Task: Gift Wrap -->
    <bpmn:task id="task3" name="Gift Wrap">
      <bpmn:incoming>f4</bpmn:incoming>
      <bpmn:outgoing>f7</bpmn:outgoing>
    </bpmn:task>
    
    <!-- Task: Insurance -->
    <bpmn:task id="task4" name="Insurance">
      <bpmn:incoming>f5</bpmn:incoming>
      <bpmn:outgoing>f8</bpmn:outgoing>
    </bpmn:task>
    
    <!-- Inclusive Gateway: Merge (OR) -->
    <bpmn:inclusiveGateway id="gw2" name="Merge">
      <bpmn:incoming>f6</bpmn:incoming>
      <bpmn:incoming>f7</bpmn:incoming>
      <bpmn:incoming>f8</bpmn:incoming>
      <bpmn:outgoing>f9</bpmn:outgoing>
    </bpmn:inclusiveGateway>
    
    <!-- Task: Complete Order -->
    <bpmn:task id="task5" name="Complete Order">
      <bpmn:incoming>f9</bpmn:incoming>
      <bpmn:outgoing>f10</bpmn:outgoing>
    </bpmn:task>
    
    <!-- End Event -->
    <bpmn:endEvent id="end1" name="End">
      <bpmn:incoming>f10</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="f1" sourceRef="start1" targetRef="task1" />
    <bpmn:sequenceFlow id="f2" sourceRef="task1" targetRef="gw1" />
    <bpmn:sequenceFlow id="f3" name="Standard" sourceRef="gw1" targetRef="task2" />
    <bpmn:sequenceFlow id="f4" name="Gift Wrap" sourceRef="gw1" targetRef="task3" />
    <bpmn:sequenceFlow id="f5" name="Insurance" sourceRef="gw1" targetRef="task4" />
    <bpmn:sequenceFlow id="f6" sourceRef="task2" targetRef="gw2" />
    <bpmn:sequenceFlow id="f7" sourceRef="task3" targetRef="gw2" />
    <bpmn:sequenceFlow id="f8" sourceRef="task4" targetRef="gw2" />
    <bpmn:sequenceFlow id="f9" sourceRef="gw2" targetRef="task5" />
    <bpmn:sequenceFlow id="f10" sourceRef="task5" targetRef="end1" />
    
  </bpmn:process>
</bpmn:definitions>
