<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   id="simple-3-lane-process">
  <bpmn:process id="Process_1">
    <!-- Start Event -->
    <bpmn:startEvent id="start1" name="Start">
      <bpmn:outgoing>flow1</bpmn:outgoing>
    </bpmn:startEvent>
    <!-- XOR Gateway (Split) -->
    <bpmn:exclusiveGateway id="xor1" name="Decision">
      <bpmn:incoming>flow1</bpmn:incoming>
      <bpmn:outgoing>flow2</bpmn:outgoing>
      <bpmn:outgoing>flow3</bpmn:outgoing>
      <bpmn:outgoing>flow4</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <!-- Task 1 (Management Lane) -->
    <bpmn:task id="task1" name="Review Request">
      <bpmn:incoming>flow2</bpmn:incoming>
      <bpmn:outgoing>flow5</bpmn:outgoing>
    </bpmn:task>
    <!-- Task 2 (IT Lane) -->
    <bpmn:task id="task2" name="Implement Solution">
      <bpmn:incoming>flow3</bpmn:incoming>
      <bpmn:outgoing>flow6</bpmn:outgoing>
    </bpmn:task>
    <!-- Task 3 (Finance Lane) -->
    <bpmn:task id="task3" name="Process Payment">
      <bpmn:incoming>flow4</bpmn:incoming>
      <bpmn:outgoing>flow7</bpmn:outgoing>
    </bpmn:task>
    <!-- End Event -->
    <bpmn:endEvent id="end1" name="End">
      <bpmn:incoming>flow5</bpmn:incoming>
      <bpmn:incoming>flow6</bpmn:incoming>
      <bpmn:incoming>flow7</bpmn:incoming>
    </bpmn:endEvent>
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="flow1" sourceRef="start1" targetRef="xor1"/>
    <bpmn:sequenceFlow id="flow2" sourceRef="xor1" targetRef="task1"/>
    <bpmn:sequenceFlow id="flow3" sourceRef="xor1" targetRef="task2"/>
    <bpmn:sequenceFlow id="flow4" sourceRef="xor1" targetRef="task3"/>
    <bpmn:sequenceFlow id="flow5" sourceRef="task1" targetRef="end1"/>
    <bpmn:sequenceFlow id="flow6" sourceRef="task2" targetRef="end1"/>
    <bpmn:sequenceFlow id="flow7" sourceRef="task3" targetRef="end1"/>
    <!-- Lanes -->
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="lane1" name="Management">
        <bpmn:flowNodeRef>start1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>xor1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>end1</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="lane2" name="IT">
        <bpmn:flowNodeRef>task2</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="lane3" name="Finance">
        <bpmn:flowNodeRef>task3</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane bpmnElement="Process_1">
      <bpmndi:BPMNShape id="LaneSet_1_di" bpmnElement="LaneSet_1" isHorizontal="true">
        <dc:Bounds x="180" y="80" width="738" height="180" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="lane1_di" bpmnElement="lane1" isHorizontal="true">
        <dc:Bounds x="180" y="260" width="738" height="180" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="lane2_di" bpmnElement="lane2" isHorizontal="true">
        <dc:Bounds x="180" y="440" width="738" height="180" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="lane3_di" bpmnElement="lane3" isHorizontal="true">
        <dc:Bounds x="180" y="620" width="738" height="180" />
      </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="start1">
      <dc:Bounds x="232" y="332" width="36" height="36"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="222" y="373" width="56" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="end1">
      <dc:Bounds x="832" y="332" width="36" height="36"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="822" y="307" width="56" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="task1">
      <dc:Bounds x="600" y="310" width="100" height="80"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="590" y="395" width="120" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="task2">
      <dc:Bounds x="600" y="490" width="100" height="80"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="590" y="575" width="120" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="task3">
      <dc:Bounds x="600" y="670" width="100" height="80"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="590" y="755" width="120" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="xor1">
      <dc:Bounds x="425" y="325" width="50" height="50"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="360" y="325" width="80" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNEdge bpmnElement="flow1">
      <di:waypoint x="268" y="350"/>
      <di:waypoint x="425" y="350"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow2">
      <di:waypoint x="475" y="350"/>
      <di:waypoint x="600" y="350"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow3">
      <di:waypoint x="450" y="375"/>
      <di:waypoint x="450" y="530"/>
      <di:waypoint x="600" y="530"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow4">
      <di:waypoint x="450" y="375"/>
      <di:waypoint x="450" y="710"/>
      <di:waypoint x="600" y="710"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow5">
      <di:waypoint x="700" y="350"/>
      <di:waypoint x="832" y="350"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow6">
      <di:waypoint x="700" y="530"/>
      <di:waypoint x="850" y="530"/>
      <di:waypoint x="850" y="368"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="flow7">
      <di:waypoint x="700" y="710"/>
      <di:waypoint x="850" y="710"/>
      <di:waypoint x="850" y="368"/>
    </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
