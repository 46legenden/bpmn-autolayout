<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   id="Definitions_1"
                   targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="lane1" name="Processing Lane">
        <bpmn:flowNodeRef>start1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>gw1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>task2</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>end1</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <!-- Start Event -->
    <bpmn:startEvent id="start1" name="Start">
      <bpmn:outgoing>f1</bpmn:outgoing>
    </bpmn:startEvent>
    <!-- Task 1: Process Request -->
    <bpmn:task id="task1" name="Process Request">
      <bpmn:incoming>f1</bpmn:incoming>
      <bpmn:incoming>f4</bpmn:incoming>
      <bpmn:outgoing>f2</bpmn:outgoing>
    </bpmn:task>
    <!-- Gateway: Check Result -->
    <bpmn:exclusiveGateway id="gw1" name="Valid?">
      <bpmn:incoming>f2</bpmn:incoming>
      <bpmn:outgoing>f3</bpmn:outgoing>
      <bpmn:outgoing>f4</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <!-- Task 2: Complete -->
    <bpmn:task id="task2" name="Complete">
      <bpmn:incoming>f3</bpmn:incoming>
      <bpmn:outgoing>f5</bpmn:outgoing>
    </bpmn:task>
    <!-- End Event -->
    <bpmn:endEvent id="end1" name="End">
      <bpmn:incoming>f5</bpmn:incoming>
    </bpmn:endEvent>
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="f1" sourceRef="start1" targetRef="task1"/>
    <bpmn:sequenceFlow id="f2" sourceRef="task1" targetRef="gw1"/>
    <bpmn:sequenceFlow id="f3" name="Yes" sourceRef="gw1" targetRef="task2"/>
    <bpmn:sequenceFlow id="f4" name="No (Retry)" sourceRef="gw1" targetRef="task1"/>
    <bpmn:sequenceFlow id="f5" sourceRef="task2" targetRef="end1"/>
  </bpmn:process>
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="pool" name="Simple Loop Process" processRef="Process_1"/>
  </bpmn:collaboration>
    <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane bpmnElement="Process_1">
      <bpmndi:BPMNShape id="pool_di" bpmnElement="pool" isHorizontal="true">
        <dc:Bounds x="150" y="280" width="968" height="160" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="LaneSet_1_di" bpmnElement="LaneSet_1" isHorizontal="true">
        <dc:Bounds x="180" y="80" width="938" height="180" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="lane1_di" bpmnElement="lane1" isHorizontal="true">
        <dc:Bounds x="180" y="260" width="938" height="180" />
      </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="start1">
      <dc:Bounds x="232" y="332" width="36" height="36"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="222" y="373" width="56" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="end1">
      <dc:Bounds x="1032" y="332" width="36" height="36"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="1022" y="373" width="56" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="task1">
      <dc:Bounds x="400" y="310" width="100" height="80"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="390" y="395" width="120" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="task2">
      <dc:Bounds x="800" y="310" width="100" height="80"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="790" y="395" width="120" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape bpmnElement="gw1">
      <dc:Bounds x="625" y="325" width="50" height="50"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="560" y="325" width="80" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNEdge bpmnElement="f1">
      <di:waypoint x="268" y="350"/>
      <di:waypoint x="400" y="350"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="f2">
      <di:waypoint x="500" y="350"/>
      <di:waypoint x="625" y="350"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="f3">
      <di:waypoint x="675" y="350"/>
      <di:waypoint x="800" y="350"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="675" y="330" width="50" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="f4">
      <di:waypoint x="650" y="325"/>
      <di:waypoint x="650" y="285"/>
      <di:waypoint x="450" y="285"/>
      <di:waypoint x="450" y="310"/>
      <bpmndi:BPMNLabel>
        <dc:Bounds x="650" y="305" width="80" height="20" />
      </bpmndi:BPMNLabel>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge bpmnElement="f5">
      <di:waypoint x="900" y="350"/>
      <di:waypoint x="1032" y="350"/>
    </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
